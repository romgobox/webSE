[uwsgi]
# Сделать пользователя uwsgi sudo adduser uwsgi
# назначить его владельцем для папки uwsgi/  
uid = 1001
gid = 1001
master = true
processes = 2
chmod-socket = 666

virtualenv = /path/to/virtual/venvdir
pythonpath = /path/to/virtual/venvdir/project
pidfile = /path/to/virtual/venvdir/project/uwsgi/webamr.pid
socket = /path/to/virtual/venvdir/project/uwsgi/webamr.sock
module = run
callable = app
logdate = true

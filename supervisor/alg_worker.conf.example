[program:alg_worker]
user=romgo
process_name=%(program_name)s_%(process_num)02d
numprocs=5
directory=/path/to/virtual/venvdir/project/
command=/path/to/virtual/venvdir/bin/python2.7 alg_worker.py
autostart=true
autorestart=true
stderr_logfile=/var/log/supervisor/alg_worker_err.log
stdout_logfile=/var/log/supervisor/alg_worker_out.log
